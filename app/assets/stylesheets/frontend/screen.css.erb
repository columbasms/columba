:after {
  -moz-box-sizing: content-box;
  -webkit-box-sizing: content-box;
  -o-box-sizing: content-box;
  -ms-box-sizing: content-box;
  box-sizing: content-box;
}
html {
  overflow-y: scroll;
  min-height: 100%;
  margin: 0 0 1px;
  font-size: 100.01%;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -moz-osx-font-smoothing: grayscale;
  -webkit-overflow-scrolling: touch;
}
body {
  min-height: 100%;
  padding-top: 28px;
  background: #e6e6e6;
  font-size: 62.5%;
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
}
body,
textarea,
input,
select,
option,
button {
  color: #929292;
  font-family: 'Open Sans', Arial, Helvetica, sans-serif;
  line-height: 1.675;
}
li,
dt,
dd,
p,
figure,
th,
td,
caption,
legend,
pre {
  font-size: 1.5em;
}
li *,
dt *,
dd *,
p *,
figure *,
th *,
td *,
legend * {
  font-size: 1em;
}
ul,
ol,
dl,
p,
figure,
table,
pre,
h1,
h2,
h3,
h4,
h5,
h6,
legend {
  margin-bottom: 20px;
}
article,
aside,
details,
div,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
summary {
